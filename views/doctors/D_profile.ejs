<!doctype html>
<head>
   <!-- <link rel="stylesheet" type="text/css href= "bootstrap-4.5.3-dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="P_profile.css"> -->

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="/css/style.css">
    <link rel="stylesheet" href="/css/PD_profile.css">
    <!-- font awesome cdn link  -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css"/>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.0/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-KyZXEAg3QhqLMpG8r+8fhAXLRk2vvoC2f3B09zVXn8CA5QIVfZOJ3BCsw2P0p/We" crossorigin="anonymous"/>
        <link rel="stylesheet" href="https://pro.fontawesome.com/releases/v5.10.0/css/all.css" integrity="sha384-AYmEC3Yw5cVb3ZcuHtOA93w35dYTsvhLPVnYs9eStHfGJvOvKxVfELGroGkvsg+p" crossorigin="anonymous"/>
    

</head>
<body>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.0/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-U1DAWAznBHeqEIlVSCgzq+c9gqGAJn5c/t99JyeKa9xxaYpSvHU5awsuZVVFIhvj"
    crossorigin="anonymous"></script>
    

    <div class="container">
<div class="card mb-3 content">
<h1 class="profhead">PROFILE</h1>
<br>

<div class="card-body">
<div class="row">
    <div class="col-md-3">
        <h4>Full Name</h4>
    </div>
    <div class=col-md-9 text-secondary>
        <%= firstname%> <%= lastname%>
    </div>
</div>
<hr>

<div class="row">
    <div class="col-md-3">
        <h4>Age</h4>
    </div>
    <div class=col-md-9 text-secondary>
        <%= age%>
    </div>
</div>
<hr>

<div class="row">
    <div class="col-md-3">
        <h4>Gender</h4>
    </div>
    <div class=col-md-9 text-secondary>
        <%= gender%>
    </div>
</div>
<hr>

<div class="row">
    <div class="col-md-3">
        <h4>Degree-Qualification</h4>
    </div>
    <div class=col-md-9 text-secondary>
        <%= degree%>
    </div>
</div>
<hr>

<div class="row">
    <div class="col-md-3">
        <h4>Specialization</h4>
    </div>
    <div class=col-md-9 text-secondary>
        <%= speciality%>
    </div>
</div>
<hr>

<div class="row">
    <div class="col-md-3">
        <h4>Email</h4>
    </div>
    <div class=col-md-9 text-secondary>
        <%= email%>
        </div>
</div>
<hr>

<div class="row">
    <div class="col-md-3">
        <h4>Phone no.</h4>
    </div>
    <div class=col-md-9 text-secondary>
        <%= phone%>
        </div>
</div>

<hr>

<div class="row">
    <div class="col-md-3">
        <h4>Pincode.</h4>
    </div>
    <div class=col-md-9 text-secondary>
        <%= pincode%>
        </div>
</div>



</div>
</div>
<div>
<a class="backbtn" href="/D_dashboard/backto_dash">Back to Dashboard</a> </div>
    </div>
</body>
</html>